{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"source": {
		"include": "src",
		"exclude": "demo",
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"destination": "jsdoc",
		"recurse": true,
		"readme": "README.md",
		"tutorials": "tutorials/",
		"template": ""
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	}
}
